ail, sub, given_name, family_name} = user;\n        const [toProfileEdit,setToProfileEdit] = useState(false);\n        const profileEditRedirect = () => {\n                console.log(\"User added to database.\");\n                setToProfileEdit(true);       \n        }\n        const [userData, setUserData] = useState({});\n        const [skillList, setSkillList] = useState([]);\n        // After component loads this runs to check if the user is in the db if so returns their \n        // info for us to use if not creates a user in db and routes to profile-edit.js\n        // The array at the end will throw an error but stops an infinite loop and shouldn't be removed. \n        useEffect(() => {\n                API.getUser(sub)\n                .then( res => {     \n                        if(!res.data){                            \n                                API.saveUser({\n                                        _id: sub,\n                                        firstname: given_name,\n                                        lastname: family_name,\n                                        email: email\n                                        })\n                                .then(profileEditRedirect)\n                                .catch(err => console.log(err));\n                        }else{\n                                setUserData(res.data);   \n                                setSkillList(res.data.skills);                                                             \n                        }\n                        }\n                )\n                .catch(err => console.log(err))\n              },[]);\n\n        return(\n                <>\n                {/* the line below allows re-direct to profile-edit  */}\n                {toProfileEdit ? <Redirect to =\"/profile-edit\" /> : null}\n                \n                <NavBar />\n                <div className=\"userInfo\">\n                <Container>\n                        <div className=\"profileCard mb-5\">\n                                <Row className=\"justify-content-end mr-4\">\n                                        <NavLink\n                                        to=\"/profile-edit\"\n                                        exact\n                                        className=\"edit\"\n                                        activeClassName=\"router-link-exact-active\"\n                                        >\n                                        < FaPen size={15} />&ensp;Edit Profile\n                                        </NavLink>\n                                </Row>\n                                <Row>\n                                        <Col md=\"7\">\n                                        <ProfileCard city={userData.city} state={userData.state} bio={userData.bio}/>\n                                        </Col>\n                                        <Col>\n                                        <Card className=\"border-0\">\n                                     \n                                                <Card.Body className=\"skillSet card-deck mt-3\">\n                                                {skillList.map((skill,i) => (\n                                                        <span key={i}>{skill}</span>\n                                                ))}\n\n                                                </Card.Body>\n                                        </Card>\n\n                                        </Col>\n                                </Row>\n                                <Row>\n                                        <Footer />    \n                                </Row>\n                        </div>\n\n                        <Row className=\"profRev\">\n                                <h4 className=\"mt-3 pb-2 mx-auto\">Reviews</h4>\n                                <ReviewSlider />\n                        </Row>\n                </Container>\n                </div>\n                </>\n        )\n        \n}","C:\\Users\\Tasha\\bootcampRepos\\project3\\client\\src\\utils\\API.js",["122"],"import axios from \"axios\";\n\nexport default {\n  // Gets all users\n  getUsers: function() {\n    return axios.get(\"/api/users\");\n  },\n  // Gets the user with the given id\n  getUser: function(id) {\n    return axios.get(\"/api/users/\" + id);\n  },\n  // Deletes the user with the given id\n  deleteUser: function(id) {\n    return axios.delete(\"/api/users/\" + id);\n  },\n    // Updates the user with the given id\n    updateUser: function(id,data) {\n      return axios.put(\"/api/users/\" + id, data);\n    },\n  // Saves a user to the database\n  saveUser: function(userData) {\n    return axios.post(\"/api/users\", userData);\n  }\n}; ","C:\\Users\\Tasha\\bootcampRepos\\project3\\client\\src\\components\\review-slider.js",[],"C:\\Users\\Tasha\\bootcampRepos\\project3\\client\\src\\components\\nav-bar.js",[],"C:\\Users\\Tasha\\bootcampRepos\\project3\\client\\src\\components\\nav-search.js",[],"C:\\Users\\Tasha\\bootcampRepos\\project3\\client\\src\\components\\small-user.js",["123","124"],"C:\\Users\\Tasha\\bootcampRepos\\project3\\client\\src\\components\\profile-card.js",[],"C:\\Users\\Tasha\\bootcampRepos\\project3\\client\\src\\components\\footer.js",[],"C:\\Users\\Tasha\\bootcampRepos\\project3\\client\\src\\components\\review-card.js",[],"C:\\Users\\Tasha\\bootcampRepos\\project3\\client\\src\\components\\main-nav.js",[],"C:\\Users\\Tasha\\bootcampRepos\\project3\\client\\src\\components\\auth-nav.js",[],"C:\\Users\\Tasha\\bootcampRepos\\project3\\client\\src\\components\\user-rating.js",[],"C:\\Users\\Tasha\\bootcampRepos\\project3\\client\\src\\components\\authentication-button.js",[],"C:\\Users\\Tasha\\bootcampRepos\\project3\\client\\src\\components\\login-button.js",[],"C:\\Users\\Tasha\\bootcampRepos\\project3\\client\\src\\components\\logout-button.js",[],"C:\\Users\\Tasha\\bootcampRepos\\project3\\client\\src\\p